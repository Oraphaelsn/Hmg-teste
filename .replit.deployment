# Configuração de Deploy - Replit
# Domínio customizado: www.hemginternacoes.com.br
# Domínio temporário: hemg-contato.replit.app

[deployment]
build = "npm run build"
start = "npm start"
deploymentTarget = "gce"

[deployment.environment]
NODE_ENV = "production"
DATABASE_URL = "$DATABASE_URL"

# Configuração para domínio customizado
[deployment.customDomain]
domain = "www.hemginternacoes.com.br"